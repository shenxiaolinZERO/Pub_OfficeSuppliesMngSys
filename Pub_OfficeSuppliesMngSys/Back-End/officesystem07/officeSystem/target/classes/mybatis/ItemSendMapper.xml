<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="cn.fjnu.officeSystem.dao.ItemSendDao">
<insert id="insertItemSend" parameterType="cn.fjnu.officeSystem.entity.ItemSend">

insert into t_item_send(id,item_id,staff_id,num,item_type_id,item_name,measure_unit_id,spec,time,is_in_storage,flag,remark)
values(#{id},#{itemId},#{staffId},#{num},#{itemTypeId},#{itemName},#{measureUnitId},#{spec},#{time},#{isInStorage},#{flag},#{remark})


</insert>

</mapper>