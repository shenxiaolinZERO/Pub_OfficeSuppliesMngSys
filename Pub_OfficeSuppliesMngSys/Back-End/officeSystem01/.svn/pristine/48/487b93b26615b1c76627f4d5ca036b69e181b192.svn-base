package cn.fjnu.officeSystem.entity;

import java.sql.Date;

public class ProcurementApply {
	   private String id;
		
		private String staffId;//员工编号
		
		private String itemName;//物品名称
		
		private String itemTypeId;//物品分类
		
		private String itemId;
		
		private String  spec;//规格型号
		
		private Integer num;//数量（填写）
			
		private Date applyTime;//申请时间
		
		private String auditId;//审核员工id
		
		private Date auditTime;//审核时间
		
		private Short isProcuremnet;//是否采购
		
		private String reason;//拒绝理由
		
		private String remark;
		
		private Short isDisplay;//是否显示

		public String getId() {
			return id;
		}

		public void setId(String id) {
			this.id = id;
		}

		public String getStaffId() {
			return staffId;
		}

		public void setStaffId(String staffId) {
			this.staffId = staffId;
		}

		public String getItemName() {
			return itemName;
		}

		public void setItemName(String itemName) {
			this.itemName = itemName;
		}

		public String getItemTypeId() {
			return itemTypeId;
		}

		public void setItemTypeId(String itemTypeId) {
			this.itemTypeId = itemTypeId;
		}

		public String getItemId() {
			return itemId;
		}

		public void setItemId(String itemId) {
			this.itemId = itemId;
		}

		public String getSpec() {
			return spec;
		}

		public void setSpec(String spec) {
			this.spec = spec;
		}

		public Integer getNum() {
			return num;
		}

		public void setNum(Integer num) {
			this.num = num;
		}

		public Date getApplyTime() {
			return applyTime;
		}

		public void setApplyTime(Date applyTime) {
			this.applyTime = applyTime;
		}

		public Date getAuditTime() {
			return auditTime;
		}

		public void setAuditTime(Date auditTime) {
			this.auditTime = auditTime;
		}

		public Short getIsProcuremnet() {
			return isProcuremnet;
		}

		public void setIsProcuremnet(Short isProcuremnet) {
			this.isProcuremnet = isProcuremnet;
		}

		public String getRemark() {
			return remark;
		}

		public void setRemark(String remark) {
			this.remark = remark;
		}
		

		public Short getIsDisplay() {
			return isDisplay;
		}

		public void setIsDisplay(Short isDisplay) {
			this.isDisplay = isDisplay;
		}

		
		public ProcurementApply() {
			super();
			// TODO Auto-generated constructor stub
		}

		public String getAuditId() {
			return auditId;
		}

		public void setAuditId(String auditId) {
			this.auditId = auditId;
		}

		public String getReason() {
			return reason;
		}

		public void setReason(String reason) {
			this.reason = reason;
		}

		public ProcurementApply(String id, String staffId, String itemName, String itemTypeId, String itemId,
				String spec, Integer num, Date applyTime, String auditId, Date auditTime, Short isProcuremnet,
				String reason, String remark, Short isDisplay) {
			super();
			this.id = id;
			this.staffId = staffId;
			this.itemName = itemName;
			this.itemTypeId = itemTypeId;
			this.itemId = itemId;
			this.spec = spec;
			this.num = num;
			this.applyTime = applyTime;
			this.auditId = auditId;
			this.auditTime = auditTime;
			this.isProcuremnet = isProcuremnet;
			this.reason = reason;
			this.remark = remark;
			this.isDisplay = isDisplay;
		}

		@Override
		public String toString() {
			return "ProcurementApply [id=" + id + ", staffId=" + staffId + ", itemName=" + itemName + ", itemTypeId="
					+ itemTypeId + ", itemId=" + itemId + ", spec=" + spec + ", num=" + num + ", applyTime=" + applyTime
					+ ", auditId=" + auditId + ", auditTime=" + auditTime + ", isProcuremnet=" + isProcuremnet
					+ ", reason=" + reason + ", remark=" + remark + ", isDisplay=" + isDisplay + "]";
		}

		
		
		
	

}
